<h1>Dashboard</h1>
<br />
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Posts</th>
      <th>Groups</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.posts.count %></td>
        <td><%= user.groups.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
